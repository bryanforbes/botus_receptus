{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>bryanforbes/python-workflows:library"
  ],
  "rangeStrategy": "bump",
  "ignoreDeps": [
    "python"
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "packageRules": [
    {
      "groupName": "asyncpg packages",
      "matchPackageNames": [
        "asyncpg",
        "asyncpg-stubs"
      ]
    }
  ]
}
